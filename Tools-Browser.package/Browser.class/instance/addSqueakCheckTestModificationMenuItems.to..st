SWT014-Browsererweiterung
addSqueakCheckTestModificationMenuItems: aCompiledMethod to: aMenu

	|subMenu|
	subMenu := MenuMorph new.
	subMenu add: 'open' target: self selector: #openCompiledMethod: argument: aCompiledMethod.
	subMenu balloonTextForLastItem: 'open test in browser'.
	subMenu add: 'run' target: self selector: #runSqueakCheckTest: argument: aCompiledMethod.
	subMenu balloonTextForLastItem: 'run'.
	subMenu add: 'delete counter examples' target: self selector: #deleteCounterExamples: argument: aCompiledMethod.
	subMenu balloonTextForLastItem: 'delete all counter examples generated by SqueakCheck'.
	aMenu add: aCompiledMethod selector subMenu: subMenu.